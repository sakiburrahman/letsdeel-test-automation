{
  "name": "letsdeel-test-automation",
  "version": "1.0.0",
  "description": "letsdell test autoamtion script using cypress and allure report",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:allure": "npm run allure:report && cypress run --env allure=true",
    "allure:report": "allure generate allure-results --clean -o allure-report",
    "cy:mocha": "cypress run --reporter mochawesome"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sakiburrahman/letsdeel-test-automation.git"
  },
  "keywords": [
    "cypress",
    "sanitytest",
    "regressiontest",
    "e2etest"
  ],
  "author": "Md. Sakibur Rahman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sakiburrahman/letsdeel-test-automation/issues"
  },
  "homepage": "https://github.com/sakiburrahman/letsdeel-test-automation#readme",
  "dependencies": {
    "cypress": "^9.2.0",
    "prettier": "^2.5.1"
  },
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.23.0",
    "cypress-multi-reporters": "^1.5.0",
    "del": "^6.0.0",
    "faker": "npm:Faker@^0.7.2",
    "Faker": "^0.7.2",
    "lodash": "^4.17.21",
    "mocha": "^9.1.3",
    "mocha-junit-reporter": "^2.0.2",
    "mochawesome": "^7.0.1",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.0.1"
  }
}
